#include <stdio.h>
void main()
{
    int a, b, c;

    printf("Escreva o valor da variavel a: ");
    scanf("%d", &a);

    printf("Escreva o valor da variavel b: ");
    scanf("%d", &b);

    if(a>b)
    {
        c = a-b;
        printf("%d",c);
    }

    else
    {
        c = b-a;
        printf("%d",c);
    }
}
