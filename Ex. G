#include <stdio.h>
void main()

{
    int a, b, c, d;

    printf ("insira o valor de A\n" );
    scanf("%d", &a);

     printf ("insira o valor de B\n");
    scanf("%d", &b);

     printf ("insira o valor de C\n");
    scanf("%d", &c);

     printf ("insira o valor de D\n");
    scanf("%d", &d);

    if (((a % 2) && (a % 3))==0)
    {
        printf("%d\n",a);
    }
     if (((b % 2) && (b % 3))==0)
    {
        printf("%d\n",b);
    }
     if (((c % 2) && (c % 3))==0)
    {
        printf("%d\n",c);
    }
     if (((d % 2) && (d % 3))==0)
    {
        printf("%d\n",d);
    }
}
