#include <stdio.h>

int main() {
    char nome[100], sexo;

    printf("Digite o nome: ");
    scanf("%s", nome);
    printf("Digite o sexo (M/F): ");
    scanf(" %c", &sexo);

    if (sexo == 'M' || sexo == 'm') {
        printf("Ilmo. Sr. %s\n", nome);
    } else if (sexo == 'F' || sexo == 'f') {
        printf("Ilma. Sra. %s\n", nome);
    } else {
        printf("Sexo informado invalido\n");
    }

    return 0;
}
