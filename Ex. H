#include <stdio.h>
void main()

{
    int a, b, c, d;

    printf ("\ninsira o valor de A: " );
    scanf("%d", &a);

    printf ("\ninsira o valor de B: ");
    scanf("%d", &b);

    printf ("\ninsira o valor de C: ");
    scanf("%d", &c);

    printf ("\ninsira o valor de D: ");
    scanf("%d", &d);

    if (((a % 2) || (a % 3))==0)
    {
        printf("%d\n",a);
    }
    if (((b % 2) || (b % 3))==0)
    {
        printf("%d\n",b);
    }
    if (((c % 2) || (c % 3))==0)
    {
        printf("%d\n",c);
    }
    if (((d % 2) || (d % 3))==0)
    {
        printf("%d",d);
    }
}
