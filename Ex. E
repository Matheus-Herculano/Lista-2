#include <stdio.h>
void main()

{
    int a, b, c, Delta, x1, x2;

    printf("Digite o coeficiente a nao nulo: ");
    scanf("%d", &a);
    printf("Digite o coeficiente b: ");
    scanf("%d", &b);
    printf("Digite o coeficiente c: ");
    scanf("%d", &c);

    Delta = (b*b)-4*a*c;

    if (Delta<0)
    {
    printf("Essa equação não tem raízes reais: ");
    }
    else
    {
        x1 = (b-(Delta)^(1/2))/(2*a);
        x2 = (b+(Delta)^(1/2))/(2*a);
        printf("As raízes da equação são x1 = %d e x2 = %d", x1, x2);
    }
}
